{
  "manifest_version": 3,
  "name": "WSN",
  "version": "1.0.0",
  "permissions": [
    "sidePanel"
  ],
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "index.html",
    "openPanelOnActionClick": true
  },
  "background": {
    "service_worker": "background.js"
  }

}